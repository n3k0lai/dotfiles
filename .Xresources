bash: X:                    96
Xft.antialias:              false
Xft.rgba:                   rgb
Xft.hinting:                true
Xft.hintstyle:              hintslight

!-------------------------------------------------------------------------------
!! URxvt settings
URxvt.depth:                32
URxvt.geometry:             90x30
URxvt.transparent:          false
URxvt.fading:               0
URxvt.loginShell:           true
URxvt.saveLines:            512
URxvt.internalBorder:       3
URxvt.lineSpace:            0
URxvt.internalBorder:       20

! Fonts
URxvt.allow_bold:           false
URxvt*font: xft:Monospace:pixelsize=14
URxvt*boldFont: xft:Monospace:pixelsize=14

! Fix font space
URxvt*letterSpace: -1

! Scrollbar
URxvt.scrollStyle:          rxvt
URxvt.scrollBar:            false

! Perl extensions
URxvt.perl-ext-common:      default,matcher,selection-to-clipboard,resize-font
URxvt.matcher.button:       1
URxvt.urlLauncher:          luakit

! copy/paste
URxvt.clipboard.autocopy:   true
URxvt.keysym.M-c:           perl:clipboard:copy
URxvt.keysim.M-v:           perl:clipboard:paste

! resize font
URxvt.keysim.C-Down:        resize-font:smaller
URxvt.keysim.C-Up:          resize-font:bigger
URxvt.iso14755:             false
URxvt.iso14755_52:          false

! Cursor
URxvt.cursorBlink:          true
URxvt.cursorUnderline:      false

! Pointer
URxvt.pointerBlank:         true

